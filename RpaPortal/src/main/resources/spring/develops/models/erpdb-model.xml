		<typeAlias alias="botCostDivideVO" type="com.kcc.biz.model.BotCostDivideVO" />		
		<!-- BOT 키스콘 하도급변경계약 등록업무 -->
		<typeAlias alias="botKisconConstVO" type="com.kcc.biz.model.BotKisconConstVO" />
		<!-- BOT 기타영수증 세금코드 변경업무 -->
		<typeAlias alias="botEtcTaxVO" type="com.kcc.biz.model.BotEtcTaxVO" />
		<!-- BOT 그룹사 인사발령 수집 업무 -->
		<typeAlias alias="botPersonAppointVO" type="com.kcc.biz.model.BotPersonAppointVO" />
