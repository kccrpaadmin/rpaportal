		<typeAlias alias="crawlSystemCheckVO" type="com.kcc.biz.model.CrawlSystemCheckVO" />
		<!-- 웹크롤링 나라장터 -->
		<typeAlias alias="crawlG2bVO" type="com.kcc.biz.model.CrawlG2bVO" />
		<!-- 웹크롤링 동관 및 환율정보 -->
		<typeAlias alias="crawlKomisVO" type="com.kcc.biz.model.CrawlKomisVO" />
		<!-- 웹크롤링 발주처 직원정보 -->
		<typeAlias alias="crawlPersonInfoVO" type="com.kcc.biz.model.CrawlPersonInfoVO" />
		<!-- 웹크롤링 원자재 정보 -->
		<typeAlias alias="crawlMaterialVO" type="com.kcc.biz.model.CrawlMaterialVO" />
		<!-- 웹크롤링 사고사례 -->
		<typeAlias alias="crawlAcdVO" type="com.kcc.biz.model.CrawlAcdVO" />
		
		<!-- Ocr 요청 -->
		<typeAlias alias="ocrRequestVO" type="com.kcc.biz.model.OcrRequestVO" />
		<!-- Ocr 텍스트 추출 -->
		<typeAlias alias="ocrTextVO" type="com.kcc.biz.model.OcrTextVO" />
		<!-- Ocr 사업자등록증 추출 -->
		<typeAlias alias="ocrBusinessLicenseVO" type="com.kcc.biz.model.OcrBusinessLicenseVO" />
		
		<!-- BOT 스케쥴 -->
		<typeAlias alias="botScheduleVO" type="com.kcc.biz.model.BotScheduleVO" />
		<!-- BOT 요청 -->
		<typeAlias alias="botRequestVO" type="com.kcc.biz.model.BotRequestVO" />
		<!-- BOT 송금확인증 발급업무 -->
		<typeAlias alias="botMoneySendVO" type="com.kcc.biz.model.BotMoneySendVO" />
