		<typeAlias alias="botEngineerVO" type="com.kcc.biz.model.BotEngineerVO" />
		<!-- BOT 나라장터 입찰 예정공사 변경사항 수집 업무 -->
		<typeAlias alias="botBidChangeVO" type="com.kcc.biz.model.BotBidChangeVO" />
		
		
	</typeAliases>
	
</configuration>