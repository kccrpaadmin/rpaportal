<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.AccessRepository">
	
	<insert id="createAccess" statementType="CALLABLE" parameterType="accessVO">
		<![CDATA[
			{ 
	            call PRA_Access_createAccess (
	            	#{UserId}
	            	,#{RoleType}
	            	,#{AccessUrl}
	            	,#{AccessIp}
	            	,#{AccessDevice}
	            	,#{Params}
	            	,#{LoginYn}
	            	,#{EmpNo}
            	)
			}
		]]>
	</insert>
	
	<insert id="createAccessFail" statementType="CALLABLE" parameterType="accessVO">
		<![CDATA[
			{ 
	            call PRA_Access_createAccessFail (
            		#{AccessUrl}
	            	,#{AccessIp}
	            	,#{AccessDevice}
	            	,#{UserId}
	            	,#{Pwd}
            	)
			}
		]]>
	</insert>
	
</mapper>