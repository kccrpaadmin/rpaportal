<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.BotMoneySendRepository">
		
	<select id="listBotMoneySendList" statementType="CALLABLE" parameterType="botMoneySendVO" resultType="botMoneySendVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotMoneySendList (
            		#{RequestNo}
            	)
			}
		]]>
	</select>
	
	<select id="listBotMoneySendManageSearchVendor" statementType="CALLABLE" parameterType="botMoneySendVO" resultType="botMoneySendVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotMoneySendManageSearchVendor (
            		#{VendorNm}
            	)
			}
		]]>
	</select>
	
	<select id="listBotMoneySendManage" statementType="CALLABLE" parameterType="botMoneySendVO" resultType="botMoneySendVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotMoneySendManage (
            		#{VendorCd},
            	    #{GlDate},
            	    #{SendAmt}
            	)
			}
		]]>
	</select>
	
	<select id="listBotMoneySendManageSendAmt" statementType="CALLABLE" parameterType="botMoneySendVO" resultType="botMoneySendVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotMoneySendManageSendAmt (
            		#{CheckId}
            	)
			}
		]]>
	</select>

</mapper>