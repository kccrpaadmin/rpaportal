<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.HomeRepository">

	<select id="listTimeLine" statementType="CALLABLE" parameterType="homeVO" resultType="homeVO">
		<![CDATA[
			{ 
	            call PRA_Home_listTimeLine (
            		#{CallDate}
            		,#{Ord}
            	)
			}
		]]>
	</select>
	
	<select id="listDeptRunTime" statementType="CALLABLE" parameterType="homeVO" resultType="homeVO">
		<![CDATA[
			{ 
	            call PRA_Home_listDeptRunTime (
            		#{WorkTypeCd}
            	)
			}
		]]>
	</select>
	
	<select id="getTaskRunTime" statementType="CALLABLE" resultType="homeVO">
		<![CDATA[
			{ 
	            call PRA_Home_getTaskRunTime
			}
		]]>
	</select>
	
	<select id="getNoticeTask" statementType="CALLABLE" resultType="homeVO">
		<![CDATA[
			{ 
	            call PRA_Home_getNoticeTask
			}
		]]>
	</select>
	
	<select id="listBotRunTime" statementType="CALLABLE" resultType="homeVO">
		<![CDATA[
			{ 
	            call PRA_Home_listBotRunTime
			}
		]]>
	</select>
	
	<select id="listBotNumberTime" statementType="CALLABLE" resultType="homeVO">
		<![CDATA[
			{ 
	            call PRA_Home_listBotNumberTime
			}
		]]>
	</select>
	
</mapper>