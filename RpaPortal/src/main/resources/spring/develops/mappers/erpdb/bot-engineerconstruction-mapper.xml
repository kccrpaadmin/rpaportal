<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.BotEngineerConstructionRepository">
	
	<select id="listBotEngineerConstructionManage" statementType="CALLABLE" parameterType="botEngineerConstructionVO" resultType="botEngineerConstructionVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotEngineerConstructionManage
	            (
	               #{OrgTypeCd}
            	   ,#{DutyNm}
            	   ,#{UserNm}
	            )
			}
		]]>
	</select>
	
	<select id="listBotEngineerConstructionResult" statementType="CALLABLE" parameterType="botEngineerConstructionVO" resultType="botEngineerConstructionVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotEngineerConstructionResult
	            (
	               #{RequestNo}
	            )
			}
		]]>
	</select>
	
</mapper>