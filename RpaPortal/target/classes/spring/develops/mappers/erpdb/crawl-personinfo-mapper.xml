<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.CrawlPersonInfoRepository">
	
	<select id="listCrawlPersonInfo" statementType="CALLABLE" parameterType="crawlPersonInfoVO" resultType="crawlPersonInfoVO">
		<![CDATA[
			{ 
	            call PRA_Crawl_listCrawlPersonInfo (
            		#{RequestNo}
            	)
			}
		]]>
	</select>
	
	<select id="listCrawlPersonInfoManage" statementType="CALLABLE" parameterType="crawlPersonInfoVO" resultType="crawlPersonInfoVO">
		<![CDATA[
			{ 
	            call PRA_Crawl_listCrawlPersonInfoManage (
	            	#{MenuId}
            		,#{OrgNm}
            		,#{DeptNm}
            		,#{UserNm}
            		,#{DutyNm}
            		,#{Task}
            	)
			}
		]]>
	</select>
	
</mapper>