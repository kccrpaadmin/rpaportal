<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.BotScheduleRepository">
	
	<insert id="createBotSchedule" statementType="CALLABLE" parameterType="botScheduleVO">
		<![CDATA[
			{ 
	            call PRA_Bot_createBotSchedule (
	            	#{MenuId}
	            	,#{Monthz}
	            	,#{Dayz}
	            	,#{Weekz}
	            	,#{WeekDayz}
	            	,#{StartTime}
	            	,#{ExpDate}
	            	,#{EmpNo}
            	)
			}
		]]>
	</insert>
	
	<delete id="deleteBotSchedule" statementType="CALLABLE" parameterType="botScheduleVO">
		<![CDATA[
			{ 
	            call PRA_Bot_deleteBotSchedule (
	            	#{MenuId}
	            	,#{SeqStr}
            	)
			}
		]]>
	</delete>
	
	<select id="listBotSchedule" statementType="CALLABLE" parameterType="botScheduleVO" resultType="botScheduleVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotSchedule (
            		#{MenuId}
            	)
			}
		]]>
	</select>
	
	<select id="listBotScheduleMenu" statementType="CALLABLE" parameterType="botScheduleVO" resultType="botScheduleVO">
		<![CDATA[
			{ 
	            call PRA_Bot_listBotScheduleMenu (
            		#{CallDate}
            	)
			}
		]]>
	</select>
	
</mapper>