<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.OcrTextRepository">
	
	<insert id="createOcrText" statementType="CALLABLE" parameterType="ocrTextVO">
		<![CDATA[
			{ 
	            call PRA_Ocr_createOcrText (
	            	#{RequestNo}
	            	,#{Seq}
	            	,#{OcrData}
	            	,#{ErrorMsg}
	            	,#{ActionCd}
	            	,#{EmpNo}
            	)
			}
		]]>
	</insert>	
	
	<select id="listOcrText" statementType="CALLABLE" parameterType="ocrTextVO" resultType="ocrTextVO">
		<![CDATA[
			{ 
	            call PRA_Ocr_listOcrText (
	            	#{RequestNo}
            	)
			}
		]]>
	</select>
	
	<select id="getOcrText" statementType="CALLABLE" parameterType="ocrTextVO" resultType="ocrTextVO">
		<![CDATA[
			{ 
	            call PRA_Ocr_getOcrText (
	            	#{RequestNo}
	            	,#{Seq}
            	)
			}
		]]>
	</select>
	
</mapper>