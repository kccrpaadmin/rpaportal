<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.AttFileRepository">
	
	<insert id="createAtt" statementType="CALLABLE" parameterType="attFileVO">
		<![CDATA[
			{ 
	            call PRA_AttFile_createAtt (
	            	#{AttId}
	            	,#{MenuId}
	            	,#{EmpNo}
            	)
			}
		]]>
	</insert>
	
	<insert id="createAttFile" statementType="CALLABLE" parameterType="attFileVO">
		<![CDATA[
			{ 
	            call PRA_AttFile_createAttFile (
            		#{AttId}
	            	,#{Seq}
	            	,#{FilePath}
	            	,#{FileNm}
	            	,#{SaveFileNm}
	            	,#{FileSize}
	            	,#{EmpNo}
            	)
			}
		]]>
	</insert>
	
	<select id="listAttFile" statementType="CALLABLE" parameterType="attFileVO" resultType="attFileVO">
		<![CDATA[
			{ 
	            call PRA_AttFile_listAttFile (
            		#{AttId}
            	)
			}
		]]>
	</select>
	
	<select id="getAttFile" statementType="CALLABLE" parameterType="attFileVO" resultType="attFileVO">
		<![CDATA[
			{ 
	            call PRA_AttFile_getAttFile (
            		#{AttId}
            		,#{Seq}
            	)
			}
		]]>
	</select>
		
</mapper>