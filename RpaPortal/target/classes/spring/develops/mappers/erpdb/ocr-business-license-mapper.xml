<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kcc.biz.dao.OcrBusinessLicenseRepository">
	
	<insert id="createOcrBusinessLicense" statementType="CALLABLE" parameterType="ocrBusinessLicenseVO">
		<![CDATA[
			{ 
	            call PRA_Ocr_createOcrBusinessLicense (
	            	#{RequestNo}
	            	,#{Seq}
	            	,#{TemplateId}
	            	,#{TemplateNm}
	            	,#{BizNo}
	            	,#{VendorNm}
	            	,#{RepNm}
	            	,#{CorpNo}
	            	,#{Addr}
	            	,#{BizType}
	            	,#{BizKind}
	            	,#{ErrorMsg}
	            	,#{ActionCd}
	            	,#{EmpNo}
            	)
			}
		]]>
	</insert>	
	
	<select id="listOcrBusinessLicense" statementType="CALLABLE" parameterType="ocrBusinessLicenseVO" resultType="ocrBusinessLicenseVO">
		<![CDATA[
			{ 
	            call PRA_Ocr_listOcrBusinessLicense (
	            	#{RequestNo}
            	)
			}
		]]>
	</select>
	
	<select id="getOcrBusinessLicense" statementType="CALLABLE" parameterType="ocrBusinessLicenseVO" resultType="ocrBusinessLicenseVO">
		<![CDATA[
			{ 
	            call PRA_Ocr_getOcrBusinessLicense (
	            	#{RequestNo}
	            	,#{Seq}
            	)
			}
		]]>
	</select>

</mapper>